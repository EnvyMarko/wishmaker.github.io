<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="/public/styleBoard.css">
    <%- include('./partials/head.ejs') %>
</head>

<%- include('./partials/adminNav.ejs') %>

<body>
    <div class="conttainer-fluid mt-5 mx-5 border col-lg-8">
        <h1 style="text-align: center;">Welcome To User's Dashboard </h1> 


        <div class="container-fluid">
            <div class="table">
              <% if (user.length > 0){ %> 
                <table class="table table-dark">
                    <thead>
                        <tr>
                            <th>User ID</th>
                            <th>Username</th>
                            <th>Password</th>
                            <th>Email</th>
                            <th>Date</th>
                        </tr>
                    </thead>
                    <tbody>
                        
                      <% user.forEach(user => { %>
                      <tr class="table-active">
                        <td><%= user.id %></td>
                        <td><%= user.user_name %></td>
                        <td><%= user.password %></td>
                        <td><%= user.email %></td>
                        <td><%= user.date %></td>
                      </tr>
                      <% }) %>
                    </tbody>
                  </table>
                  <% } else{ %>
                <%} %>
            </div>
        </div>
    </div>
    

       
</body>
</html>