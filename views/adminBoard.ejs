<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('./partials/head.ejs') %>
</head>

<%- include('./partials/adminNav.ejs') %>

<body>
    <div class="book-content container-fluid">
        <h1 style="text-align: center;">Welcome To Dashboard User <%= forUser%></h1> 
              <% if (wish.length > 0){ %> 
                <table class="table table-dark">
                    <thead>
                        <tr>
                            <th>Wish ID</th>
                            <th>Details</th>
                            <th>Img</th>
                            <th>Image</th>
                            <th>Author</th>
                        </tr>
                    </thead>
                    <tbody>
                        
                      <% wish.forEach(wish => { %>
                        
                      <tr class="table-active">
                        <td><%= wish.id %></td>
                        <td><%= wish.Wishitem %></td>
                        <td><%= wish.details %></td>
                        <td><%= wish.wishimg %></td>
                        <td><%= wish.author %></td>
                      </tr>
                      <% }) %>
                    </tbody>
                  </table>
                  <% } else{ %>
                <%} %>
    </div>
</body>
</html>